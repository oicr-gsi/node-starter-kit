{
  "name": "node-starter-kit",
  "version": "1.0.0",
  "description": "Standard dependences for Node projects on the OICR GSI team",
  "main": "index.js",
  "scripts": {
    "test": "mocha --exit",
    "pretty-quick": "pretty-quick",
    "lint": "eslint --print-config .eslintrc | eslint-config-prettier-check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty-quick --staged",
      "pre-push": "npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oicr-gsi/node-starter-kit.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oicr-gsi/node-starter-kit/issues"
  },
  "homepage": "https://github.com/oicr-gsi/node-starter-kit#readme",
  "dependencies": {
    "dotenv": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "husky": "^0.14.3",
    "mocha": "^5.0.0",
    "prettier": "^1.10.2",
    "pretty-quick": "^1.2.3"
  }
}
